{
  "engines": {
    "node": "14.17",
    "yarn": "^1.22.10"
  },
  "scripts": {
    "start": "node build/server.js",
    "build": "concurrently -n srv,cli 'npm:build-server' 'npm:build-client'",
    "build-client": "NODE_ENV=production webpack --config webpack/webpack.config.client.production.js",
    "build-server": "NODE_ENV=production webpack --config webpack/webpack.config.server.js",
    "watch": "concurrently -n wds,srv 'npm:watch-client' 'npm:watch-server'",
    "watch-client": "NODE_ENV=development webpack serve --config webpack/webpack.config.client.development.js",
    "watch-server": "concurrently -n bld,run 'npm:watch-server-build' 'npm:watch-server-run'",
    "watch-server-build": "NODE_ENV=development webpack --config webpack/webpack.config.server.js --watch",
    "watch-server-run": "NODE_ENV=development nodemon -w build/server.js build/server.js",
    "heroku-postbuild": "yarn build",
    "test": "NODE_ENV=test jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "codegen": "graphql-codegen --config codegen.yml && yarn prettier --write src/client/data/graphql.tsx",
    "storybook": "start-storybook -p 6008",
    "build-storybook": "build-storybook",
    "download-translations": "./bin/download-translations.sh"
  },
